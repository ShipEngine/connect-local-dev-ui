{
  "name": "@shipengine/shipengine-integration-platform-local-dev-ui",
  "version": "0.0.2",
  "main": "build/index.js",
  "scripts": {
    "build": "react-scripts build && tsc -p ./scripts/post-build/tsconfig.json",
    "bump": "bump --tag --push --all",
    "clean": "shx rm -rf coverage build",
    "format": "prettier --write src/**/*.ts{,x}",
    "lint": "eslint src/**/*.ts{,x}",
    "release": "npm run upgrade && npm run clean && npm run lint && CI=true react-scripts test && npm run bump",
    "start": "PORT=4000 react-scripts start",
    "test": "react-scripts test",
    "upgrade": "npm-check -u && npm audit fix"
  },
  "dependencies": {
    "antd": "^4.6.2",
    "node-sass": "^4.14.1",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-router-dom": "^5.2.0",
    "react-scripts": "^3.4.3"
  },
  "devDependencies": {
    "@jsdevtools/version-bump-prompt": "^6.1.0",
    "@testing-library/jest-dom": "^5.11.4",
    "@testing-library/react": "^11.0.2",
    "@testing-library/user-event": "^12.1.3",
    "@types/jest": "^26.0.13",
    "@types/node": "^14.6.4",
    "@types/react": "^16.9.49",
    "@types/react-dom": "^16.9.0",
    "@types/react-router-dom": "^5.1.5",
    "@typescript-eslint/eslint-plugin": "^4.0.1",
    "@typescript-eslint/parser": "^4.0.1",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-jest": "^24.0.0",
    "eslint-plugin-prettier": "^3.1.4",
    "npm-check": "^5.9.2",
    "prettier": "^2.1.1",
    "shx": "^0.3.2",
    "typescript": "^4.0.2"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
